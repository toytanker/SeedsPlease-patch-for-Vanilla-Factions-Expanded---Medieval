<Defs>

  <!-- Recipes_Plants_VFEM.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Grape</defName>
    <label>extract grape seeds</label>
    <description>Extract seeds from Grapes.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>VFEM_RawGrapes</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>VFEM_RawGrapes</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Grapes>3</Seed_Grapes>
    </products>
  </RecipeDef>
  
</Defs>
